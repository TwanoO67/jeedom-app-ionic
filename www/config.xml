<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ionic.starter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" versionCode="1">
    <name>JeedomIonicApp</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />

    <plugin name="ionic-plugin-keyboard" source='npm' />
    <plugin name="cordova-plugin-whitelist" source='npm' />
    <plugin name="cordova-plugin-device" source='npm' />
    <plugin name="cordova-plugin-splashscreen" source='npm' />
    <plugin name="cordova-plugin-ionic-webview" source='npm' />
</widget>